      <ui:InvokeCode ContinueOnError="{x:Null}" sap2010:Annotation.AnnotationText="Response dosyasının içindeki Xml dosyasında bulunan&#xA;creditTypesList.creditType 'ı 120 ve 200 olan node'ları alır.    &#xA;&#xA;kv = KısaVadeli: Amount06 +Amount612 &#xA;uv = Uzun Vadeli: totalExposure - KısaVadeli " Code="XDocument doc = in_doc;&#xA;XNamespace ns2 = &quot;http://isbank.com/FinancialMgmt/CustCreditDecisioning/CMAFinancialsIntegration/Service/V1&quot;;&#xA;&#xA;// Sadece 120 ve 200'ü hesaplar içinde dönmesini bekliyoruz&#xA;var creditValue = new List&lt;string&gt;() { &quot;120&quot;, &quot;200&quot;};&#xA;&#xA;string exposureAmount06 = null;&#xA;string exposureAmount612 = null;&#xA;string totalExposure = null;&#xA;&#xA;//Eğer 120 ve 200 nolu hesaplar yoksa uv ve kv değerleri default olarak &quot;0&quot; olur&#xA;//uv = Uzun Vadeli = totalExposure - KısaVadeli&#xA;//kv = KısaVadeli = exposureAmount06 + exposureAmount612 &#xA;int kv = 0;&#xA;int uv = 0;&#xA;&#xA;//Daha önceden liste (creditValue isimli liste) halinde verilmiş credit type'lar içinde dönen loop (120 ve 200 nolu hesaplara bakar)          &#xA;foreach (var value in creditValue)&#xA;            {&#xA;                  var creditTypesList = (from node in doc.Descendants(ns2 + &quot;creditTypesList&quot;)&#xA;                                                    where node.Element(ns2 + &quot;creditType&quot;).Value == value&#xA;                                                    select node);&#xA;&#xA;                &#xA;                //İlgili node'un içinde Total exposure, Amount06 ve Amount612 değerlerini bulur &#xA;                foreach (var element in creditTypesList)&#xA;                {&#xA;                        &#xA;                        exposureAmount06 = element.Element(ns2 + &quot;exposureAmount06&quot;).Value;                        &#xA;                        exposureAmount612 = element.Element(ns2 + &quot;exposureAmount612&quot;).Value;&#xA;                        totalExposure = element.Element(ns2 + &quot;totalExposure&quot;).Value;&#xA;&#xA;                        Console.WriteLine(&quot;Credit Type : &quot; + value);&#xA;                        Console.WriteLine(&quot;exposureAmount06 : &quot; + exposureAmount06);&#xA;                        Console.WriteLine(&quot;exposureAmount612 : &quot; + exposureAmount612);&#xA;                        Console.WriteLine(&quot;Total Exposure : &quot; + totalExposure);&#xA;&#xA;                        //kv = KısaVadeli: exposureAmount06 + exposureAmount612 &#xA;                        kv = Convert.ToInt32(exposureAmount06) + Convert.ToInt32(exposureAmount612);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;   &#xA;&#xA;                        //uv = Uzun Vadeli: totalExposure - KısaVadeli&#xA;                        uv = Convert.ToInt32(totalExposure) - kv;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;    &#xA;&#xA;                }&#xA;                     &#xA;            }&#xA;                      &#xA;out_kv = kv;&#xA;out_uv = uv;&#x9;&#x9;" DisplayName="Invoke code" sap:VirtualizedContainerService.HintSize="384,272.8" sap2010:WorkflowViewState.IdRef="InvokeCode_2" Language="CSharp">
